  "version": "0.0.2",
  "description": "Postman plugin for Syncano CLI",
  "main": ".dist/index.js",
  "scripts": {
    "build": "babel src --out-dir .dist --copy-files --extensions .js",
    "build:watch": "babel src --out-dir .dist --copy-files --extensions .js --watch",
